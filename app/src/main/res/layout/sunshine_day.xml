<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg0_fine_day">
    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/birdup_imageview"
        android:layout_centerInParent="true"
        android:src="@drawable/birdup_animationlist"/>
    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/birddown_imageview"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="500dp"
        android:src="@drawable/birddown_animationlist"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cloud_imageview"
        android:src="@drawable/fine_day_cloud1"
        android:layout_below="@id/birdup_imageview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="70dp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sunshine_imageview"
        android:src="@drawable/sunshine"
        android:layout_marginTop="175dp"
        android:layout_marginLeft="28dp" />
</RelativeLayout>
